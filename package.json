    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "rollup": "4.34.3",
    "tslib": "2.8.1",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "aerie-actions": "file:../aerie-actions"
  }
